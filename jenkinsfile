#!groovy

pipeline{    
agent {
           label "master"
        }
stages{
stage('Checkout') {
steps{       
 checkout scm
    }
}
    
    stage('Run tests') {


	steps {
                withMaven(maven : 'Maven 3') {
                    sh 'mvn install -e -Dmaven.test.skip=true'
                }
            }

               



 }   
}
}
