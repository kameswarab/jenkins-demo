#!groovy

pipeline{    
agent {
           label "CICD-${params.ENV}"
        }
stages{
stage('Checkout') {
steps{       
 checkout scm
    }
}
    
    stage('Run tests') {

             steps{
                    sh 'mvn clean test -Dmaven.test.failure.ignore=true'
	     }
               



 }   
}
}
